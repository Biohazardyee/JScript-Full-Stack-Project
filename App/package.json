    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongo": "^0.1.0",
    "mongoose": "^8.19.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "chai": "^6.2.0",
    "mocha": "^11.7.4",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.1",
    "supertest": "^7.1.4"
  }
}
